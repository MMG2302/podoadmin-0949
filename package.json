{
  "name": "sandbox-website-template",
  "description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
  "private": true,
  "dependencies": {
    "@ai-sdk/openai": "^3.0.0-beta.90",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/bcryptjs": "^3.0.0",
    "ai": "^6.0.0-beta.143",
    "atmn": "^0.0.30",
    "autumn-js": "^0.1.48",
    "base32-encode": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dedent": "^1.7.0",
    "drizzle-orm": "^0.44.7",
    "hono": "4.8.2",
    "lucide-react": "^0.555.0",
    "mathjs": "^15.1.0",
    "otpauth": "^9.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.68.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "wouter": "^3.8.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@eslint/js": "9.29.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25.0.0",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript/native-preview": "^7.0.0-dev.20251204.1",
    "@vitejs/plugin-react": "5.0.0",
    "baseline-browser-mapping": "^2.10.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "9.27.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite": "^6.0.0",
    "wrangler": "^4.68.1"
  },
  "scripts": {
    "build": "vite build",
    "cf-typegen": "wrangler types",
    "check": "tsgo && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite",
    "tunnel": "npx --yes cloudflared tunnel --url http://localhost:5173",
    "lint": "eslint .",
    "preview": "bun run build && vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "wrangler d1 migrations apply DB --remote",
    "db:seed:local": "wrangler d1 execute DB --local --file=scripts/seed-mock-users.sql && wrangler d1 execute DB --local --file=scripts/seed-clinics.sql",
    "db:seed:remote": "wrangler d1 execute DB --remote --file=scripts/seed-mock-users.sql && wrangler d1 execute DB --remote --file=scripts/seed-clinics.sql",
    "db:studio": "drizzle-kit studio",
    "db:seed-mock-users": "node scripts/seed-mock-users.cjs",
    "db:create-super-admin": "node scripts/create-super-admin.cjs",
    "setup:env": "node scripts/setup-env.js"
  },
  "type": "module",
  "tasks": {
    "bun-install": "bun install > bun-install.log 2>&1",
    "lint:log": "bun run lint > lint.log 2>&1",
    "typecheck:log": "bun run typecheck > typecheck.log 2>&1",
    "build:log": "bun run build > build.log 2>&1",
    "test:log": "bun run test > test.log 2>&1",
    "db-generate:log": "bun run db:generate > db-generate.log 2>&1",
    "db-migrate:log": "bun run db:migrate > db-migrate.log 2>&1",
    "db-seed:log": "bun run db:seed:local > db-seed.log 2>&1"
  }
}